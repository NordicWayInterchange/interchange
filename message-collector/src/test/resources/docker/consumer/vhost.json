{
  "type": "Memory",
  "name": "localhost",
  "modelVersion": "7.0",
  "exchanges": [
    {
      "name": "subscriptionExchange",
      "type": "headers",
      "durable": true,
      "durableBindings": [
         {
          "arguments": {
            "x-filter-jms-selector": "originatingCountry='SE'"
          },
          "bindingKey": "originatingCountry",
          "destination": "sp_consumer"
        }
      ]
    }
  ],
  "queues": [
    {
      "name": "sp_consumer",
      "type": "standard",
      "durable": true,
      "holdOnPublishEnabled": false,
      "owner": null
    }
  ],
  "virtualhostaccesscontrolproviders": [
    {
      "name": "default",
      "type": "RuleBased",
      "defaultResult": "DENIED",
      "rules": [
        {
          "objectType": "ALL",
          "identity": "administrators",
          "operation": "ALL",
          "outcome": "ALLOW_LOG",
          "attributes": {}
        },
        {
          "objectType": "VIRTUALHOST",
          "identity": "service-providers",
          "operation": "ACCESS",
          "outcome": "ALLOW_LOG",
          "attributes": {
            "NAME": "localhost"
          }
        },
        {
          "objectType": "QUEUE",
          "identity": "sp_consumer",
          "operation": "CONSUME",
          "outcome": "ALLOW_LOG",
          "attributes": {
            "NAME": "sp_consumer"
          }
        },
        {
          "objectType": "ALL",
          "identity": "ALL",
          "operation": "ALL",
          "outcome": "DENY_LOG",
          "attributes": {}
        }
      ]
    }
  ]
}
