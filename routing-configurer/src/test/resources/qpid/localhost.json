{
  "type": "BDB",
  "name": "localhost",
  "modelVersion": "7.0",
  "exchanges": [
    {
      "name": "nwEx",
      "type": "headers",
      "durable": true,
      "durableBindings": [
        {
          "arguments": {
            "x-filter-jms-selector": "where='NO'"
          },
          "bindingKey": "where",
          "destination": "NO-out"
        },
        {
          "arguments": {
            "x-filter-jms-selector": "where LIKE '%'"
          },
          "bindingKey": "where",
          "destination": "test-out"
        },
        {
          "arguments": {
            "x-filter-jms-selector": "where='NO' AND what='Obstruction'"
          },
          "bindingKey": "where, what",
          "destination": "NO-Obstruction"
        },
        {
          "arguments": {
            "x-filter-jms-selector": "where='SE'"
          },
          "bindingKey": "where",
          "destination": "SE-out"
        }
      ]
    }
  ],
  "queues": [
    {
      "name": "onramp",
      "type": "standard",
      "durable": true,
      "holdOnPublishEnabled": false,
      "owner": null
    },
    {
      "name": "dlqueue",
      "type": "standard",
      "durable": true,
      "holdOnPublishEnabled": false,
      "owner": null
    }
  ],
  "virtualhostaccesscontrolproviders": [
    {
      "name": "default",
      "type": "RuleBased",
      "defaultResult": "DENIED",
      "rules": [
        {
          "objectType": "ALL",
          "identity": "interchange",
          "operation": "ALL",
          "outcome": "ALLOW_LOG",
          "attributes": {}
        },
        {
          "objectType": "ALL",
          "identity": "administrators",
          "operation": "ALL",
          "outcome": "ALLOW_LOG",
          "attributes": {}
        },
        {
          "objectType": "EXCHANGE",
          "identity": "service-providers",
          "operation": "PUBLISH",
          "outcome": "ALLOW_LOG",
          "attributes": {
            "ROUTING_KEY": "onramp",
            "NAME": ""
          }
        },
        {
          "objectType": "VIRTUALHOST",
          "identity": "service-providers",
          "operation": "ACCESS",
          "outcome": "ALLOW_LOG",
          "attributes": {
            "NAME": "localhost"
          }
        },
        {
          "objectType": "QUEUE",
          "identity": "interchange",
          "operation": "CONSUME",
          "outcome": "ALLOW_LOG",
          "attributes": {
            "NAME": "onramp"
          }
        },
        {
          "objectType": "ALL",
          "identity": "ALL",
          "operation": "ALL",
          "outcome": "DENY_LOG",
          "attributes": {}
        }
      ]
    }
  ]
}
